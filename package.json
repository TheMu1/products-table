{
  "name": "products-table",
  "version": "1.0.0",
  "description": "Products table",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --config webpack.config.js --mode development",
    "build": "npm run build-css && webpack --config webpack.config.js --mode production",
    "start": "node server.js",
    "build-css": "node-sass src/frontend/styles/sass -o src/frontend/styles/css",
    "watch-css": "npm run build-css && node-sass src/frontend/styles/sass -o src/frontend/styles/css --watch --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://"
  },
  "dependencies": {
    "express": "^4.17.1",
    "highcharts": "^8.2.0",
    "highcharts-react-official": "^3.0.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "regenerator-runtime": "^0.13.3",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.1",
    "style-loader": "^1.0.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.40.2",
    "webpack-bundle-analyzer": "^3.5.1",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1",
    "node-sass": "^4.14.1"
  }
}